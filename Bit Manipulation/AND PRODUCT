 #include<bits/stdc++.h>

 using namespace std;

 #define all(x) x.begin(),x.end()
 #define ull unsigned long long int
 #define ll long long int
 #define zinga_lala ll t;cin>>t;while(t--){cp();}
 #define code_sesh_yoo return 0;
 #define yes cout<<"YES"<<endl;
 #define no  cout<<"NO"<<endl;
 void speed()
 {
     ios_base::sync_with_stdio(false);
     cin.tie(NULL);
     cout.tie(NULL);
 }
 void cp()
 {
     ll a,b,d;
     cin>>a>>b;
     d = (b-a);
     ll ans=0,f=1;
     for(int i=0;i<32;i++){
        if(d>(f<<i))continue;
        else{
            if((a&(f<<i)) && (b&(f<<i))){
                ans+=f<<i;
            }
        }
     }
     cout<<ans<<endl;
 }
 int main()
 {
     speed();
     //cp();
     zinga_lala
     code_sesh_yoo
 }

