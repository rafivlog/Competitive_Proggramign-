void fast()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
}
vector<pair<ll,ll> > adj[502];
void solve()
{
	ll n,m,a,b,w;
	cin>>n>>m;
	while(m--){
		cin>>a>>b>>w;
		adj[a].push_back({b,w});
        adj[b].push_back({a,w});
	}
    priority_queue<pair<ll,ll>, vector<pair<ll,ll> >,greater<pair<ll,ll> > > que;
    vector<ll> dist(n+1,10000007);
    que.push({0,1});
    dist[1] = 0;
    while(!que.empty()){
    	int curr = que.top().second;
    	int curr_distance = que.top().first;
    	que.pop();
    	for(auto child : adj[curr]){
    		if(curr_distance + child.second < dist[child.first]){
    			dist[child.first] = curr_distance + child.second;
    			que.push({dist[child.first],child.first});
    		}
    	}
    }
    for(int i=1;i<=n;i++){
    	cout<<dist[i]<<" ";
    }
    cout<<endl;
}
int main()
{
    fast();
    rf();

    //solve();
    khela_shuru
    khela_sesh
}
    
