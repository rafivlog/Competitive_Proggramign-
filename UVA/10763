#define ll long long
#define khela_shuru int t;cin>>t;while(t--){solve();}
#define khela_sesh return 0;
#define yes cout<<"YES"<<endl;
#define no  cout<<"NO"<<endl;
#define sp  " "
#define case(tc) cout << "Case " << tc <<": "
ll casenum = 1;
int dx[] = {-1,0,1,0}; // U R D L
