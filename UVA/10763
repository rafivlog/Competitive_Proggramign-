void rf()
{
    #ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
    #endif
}
void fast()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
}
void solve()
{
    while(1)
    {
        ll n;
        cin>>n;
        if(n==0) break;
        vp f,r;
        ll h,k;
        while(n--){
            cin>>h>>k;
            f.pb({h,k});
            r.pb({k,h});
        }
        sort(all(f));
        sort(all(r));
        // for(auto el : f){
        //     cout<<el.ff<<sp<<el.ss<<endl;
        // }
        // cout<<endl;
        // for(auto el : r){
        //     cout<<el.ff<<sp<<el.ss<<endl;
        // }
        if(f==r)
            yes
        else
            no
    }
}
int main()
{
    fast();
    rf();
    solve();
    //khela_shuru
    khela_sesh
}
