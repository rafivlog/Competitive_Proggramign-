void fast()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
}
vector<ll> adj[100001];
ll vis[100001];
ll dist[100001];
vector<ll> primes;

bool is_prime(ll n)
{
    for(ll i=2;i*i<=n;i++){
        if(n%i==0){
            return false;
        }
    }
    return true;
}
bool is_valid(ll k,ll l)
{
    ll c = 0;
    while(k){
        if((k%10) != (l%10)){
            c++;
        }
        k/=10;
        l/=10;
    }
    if(c==1){
        return true;
    }else{
        return false;
    }
}
void Graph()
{
    for(ll i=1000;i<=9999;i++){
        if(is_prime(i)){
            primes.pb(i);
        }
    }
    for(ll i=0;i<len(primes);i++){
        for(ll j=i+1;j<len(primes);j++){
            ll kk,mm;
            kk = primes[i];
            mm = primes[j];
            if(is_valid(kk,mm)){
                adj[kk].pb(mm);
                adj[mm].pb(kk);
            }
        }
    }
}
void BFS(ll src)
{
    queue<int>q;
    q.push(src);
    vis[src] = 1;
    dist[src] = 0;
    while(!q.empty())
    {
        ll curr = q.front();
        q.pop();
        for(ll child : adj[curr])
        {
            if(vis[child]==0)
            {
                q.push(child);
                dist[child] = dist[curr]+1;
                vis[child] = 1;
            }
        }
    }
}

void solve()
{
    ll a,b;
    cin>>a>>b;
    if(a==b){
        cout<<0<<endl;
        return;
    }
    for(ll i=1000;i<=9999;i++){
        dist[i] = -1;
        vis[i] = 0;
    }
    BFS(a);
    if(dist[b]==-1){
        cout<<"Impossible"<<endl;
    }else{
        cout<<dist[b]<<endl;
    }
}
int main()
{
    fast();
    rf();
    Graph();
    //solve();
    khela_shuru
    khela_sesh
}
    
